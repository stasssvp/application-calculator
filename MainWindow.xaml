        <Button x:Name="factorial" Content="x!" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="10,370,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="factorial_Click" Grid.ColumnSpan="3"/>
        <Button x:Name="sine" Content="sin" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="7,370,0,0" VerticalAlignment="Top" Width="85" Height="74" Foreground="White" Cursor="Arrow" Click="sine_Click" Grid.Column="2"/>
        <Button x:Name="cosine" Content="cos" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="93,370,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="cosine_Click" Grid.Column="2"/>
        <Button x:Name="tangent" Content="tan" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="180,370,0,0" VerticalAlignment="Top" Width="85" Height="74" Foreground="White" Cursor="Arrow" Click="tangent_Click" Grid.Column="2"/>
        <Button x:Name="euler_number" Content="e" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="266,370,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="euler_number_Click" Grid.Column="2"/>
        <Button x:Name="exponential_notation" Content="EE" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="353,370,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="exponential_notation_Click" Grid.Column="2"/>

        <Button x:Name="radian" Content="Rad" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="10,445,0,0" VerticalAlignment="Top" Width="86" Height="75" Foreground="White" Cursor="Arrow" Click="radian_Click" Grid.ColumnSpan="3"/>
