<Window x:Class="application_calculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Calculator" Height="540" Width="1060" ResizeMode="NoResize" Icon="/calculator.jpg">

    <Grid Background="#2A2731" Margin="0,0,0,-6">
        <Button x:Name="clear" Content="AC" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="440,142,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="clear_Click" Grid.Column="2"/>
        <Button x:Name="negate" Content="+/-" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="527,142,0,0" VerticalAlignment="Top" Width="85" Height="74" Foreground="White" Cursor="Arrow" Click="negate_Click" Grid.Column="2"/>
        <Button x:Name="percent" Content="%" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="613,142,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="percent_Click" Grid.Column="2"/>
        <Button x:Name="division" Content="÷" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#FFA01C" HorizontalAlignment="Left" Margin="700,142,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="division_Click" Grid.Column="2"/>

        <Button x:Name="seven" Content="7" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#69676F" HorizontalAlignment="Left" Margin="440,217,0,0" VerticalAlignment="Top" Width="86" Height="76" Foreground="White" Cursor="Arrow" Click="seven_Click" Grid.Column="2"/>
        <Button x:Name="eight" Content="8" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#69676F" HorizontalAlignment="Left" Margin="527,217,0,0" VerticalAlignment="Top" Width="85" Height="76" Foreground="White" Cursor="Arrow" Click="eight_Click" Grid.Column="2" />
        <Button x:Name="nine" Content="9" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#69676F" HorizontalAlignment="Left" Margin="613,217,0,0" VerticalAlignment="Top" Width="86" Height="76" Foreground="White" Cursor="Arrow" Click="nine_Click" Grid.Column="2"/>
        <Button x:Name="multiplication" Content="×" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#FFA01C" HorizontalAlignment="Left" Margin="700,217,0,0" VerticalAlignment="Top" Width="86" Height="76" Foreground="White" Cursor="Arrow" Click="multiplication_Click" Grid.Column="2"/>

        <Button x:Name="four" Content="4" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#69676F" HorizontalAlignment="Left" Margin="440,294,0,0" VerticalAlignment="Top" Width="86" Height="75" Foreground="White" Cursor="Arrow" Click="four_Click" Grid.Column="2"/>
        <Button x:Name="five" Content="5" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#69676F" HorizontalAlignment="Left" Margin="527,294,0,0" VerticalAlignment="Top" Width="85" Height="75" Foreground="White" Cursor="Arrow" Click="five_Click" Grid.Column="2"/>
        <Button x:Name="six" Content="6" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#69676F" HorizontalAlignment="Left" Margin="613,294,0,0" VerticalAlignment="Top" Width="86" Height="75" Foreground="White" Cursor="Arrow" Click="six_Click" Grid.Column="2"/>
        <Button x:Name="subtraction" Content="-" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#FFA01C" HorizontalAlignment="Left" Margin="700,294,0,0" VerticalAlignment="Top" Width="86" Height="75" Foreground="White" Cursor="Arrow" Click="subtraction_Click" Grid.Column="2"/>

        <Button x:Name="one" Content="1" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#69676F" HorizontalAlignment="Left" Margin="440,370,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="one_Click" Grid.Column="2"/>
        <Button x:Name="two" Content="2" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#69676F" HorizontalAlignment="Left" Margin="527,370,0,0" VerticalAlignment="Top" Width="85" Height="74" Foreground="White" Cursor="Arrow" Click="two_Click" Grid.Column="2"/>
        <Button x:Name="three" Content="3" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#69676F" HorizontalAlignment="Left" Margin="613,370,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="three_Click" Grid.Column="2"/>
        <Button x:Name="addition" Content="+" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#FFA01C" HorizontalAlignment="Left" Margin="700,370,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="addition_Click" Grid.Column="2"/>

        <Button x:Name="zero" Content="0" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#69676F" HorizontalAlignment="Left" Margin="440,445,0,0" VerticalAlignment="Top" Width="172" Height="75" Foreground="White" Cursor="Arrow" Click="zero_Click" Grid.Column="2"/>
        <Button x:Name="decimal" Content="." FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#69676F" HorizontalAlignment="Left" Margin="613,445,0,0" VerticalAlignment="Top" Width="86" Height="75" Foreground="White" Cursor="Arrow" Click="decimal_Click" Grid.Column="2"/>
        <Button x:Name="equal" Content="=" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#FFA01C" HorizontalAlignment="Left" Margin="700,445,0,0" VerticalAlignment="Top" Width="86" Height="75" Foreground="White" Cursor="Arrow" Click="equal_Click" Grid.Column="2"/>

        <TextBox x:Name="display_operation" HorizontalAlignment="Left" Height="39" Margin="450,29,0,0" BorderThickness="0" TextWrapping="Wrap" Text="" HorizontalContentAlignment="Right" VerticalContentAlignment="Center" Background="#2A2731" FontFamily="Arial Rounded MT Bold" FontSize="14px" Foreground="White" VerticalAlignment="Top" Width="311" Grid.Column="2"/>
        <TextBox x:Name="display_result" HorizontalAlignment="Left" Height="72" HorizontalContentAlignment="Right" BorderThickness="0" VerticalContentAlignment="Center" Background="#2A2731" FontSize="28px" Margin="450,69,0,0" Foreground="White" FontFamily="Arial Rounded MT Bold" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="311" Grid.Column="2"/>


        <Button x:Name="left_bracket" Content="(" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="10,142,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="left_bracket_Click" Grid.ColumnSpan="3"/>
        <Button x:Name="right_bracket" Content=")" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="7,142,0,0" VerticalAlignment="Top" Width="85" Height="74" Foreground="White" Cursor="Arrow" Click="right_bracket_Click" Grid.Column="2"/>
        <Button x:Name="memory_clear" Content="mc" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="93,142,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="memory_clear_Click" Grid.Column="2"/>
