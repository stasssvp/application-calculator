        <Button x:Name="hyperbolic_cosine" Content="cosh" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="93,445,0,0" VerticalAlignment="Top" Width="86" Height="75" Foreground="White" Cursor="Arrow" Click="hyperbolic_cosine_Click" Grid.Column="2"/>
        <Button x:Name="hyperbolic_tangent" Content="tanh" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="180,445,0,0" VerticalAlignment="Top" Width="85" Height="75" Foreground="White" Cursor="Arrow" Click="hyperbolic_tangent_Click" Grid.Column="2"/>
