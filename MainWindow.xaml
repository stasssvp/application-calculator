        <Button x:Name="random" Content="Rand" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="353,445,0,0" VerticalAlignment="Top" Width="86" Height="75" Foreground="White" Cursor="Arrow" Click="random_Click" Grid.Column="2"/>

