        <Button x:Name="memory_subtract" Content="m-" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="266,142,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="memory_subtract_Click" Grid.Column="2"/>
        <Button x:Name="memory_recall" Content="mr" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="353,142,0,0" VerticalAlignment="Top" Width="86" Height="74" Foreground="White" Cursor="Arrow" Click="division_Click" Grid.Column="2"/>

        <Button x:Name="alt_functions" Content="2ⁿᵈ" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="10,217,0,0" VerticalAlignment="Top" Width="86" Height="76" Foreground="White" Cursor="Arrow" Click="alt_functions_Click" Grid.ColumnSpan="3"/>
        <Button x:Name="square_power" Content="x²" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="7,217,0,0" VerticalAlignment="Top" Width="85" Height="76" Foreground="White" Cursor="Arrow" Click="square_power_Click" Grid.Column="2"/>
        <Button x:Name="cube_power" Content="x³" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="93,217,0,0" VerticalAlignment="Top" Width="86" Height="76" Foreground="White" Cursor="Arrow" Click="cube_power_Click" Grid.Column="2"/>
        <Button x:Name="x_power_y" Content="" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="180,217,0,0" VerticalAlignment="Top" Width="85" Height="74" Foreground="White" Cursor="Arrow" Click="x_power_y_Click" Grid.Column="2"/>
        <Button x:Name="euler_number_power" Content="eˣ" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="266,217,0,0" VerticalAlignment="Top" Width="86" Height="76" Foreground="White" Cursor="Arrow" Click="euler_number_power_Click" Grid.Column="2"/>
        <Button x:Name="ten_power" Content="10ˣ" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="353,217,0,0" VerticalAlignment="Top" Width="86" Height="76" Foreground="White" Cursor="Arrow" Click="ten_power_Click" Grid.Column="2"/>

        <Button x:Name="y_power_x" Content="" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="180,217,0,0" VerticalAlignment="Top" Width="85" Height="74" Foreground="White" Cursor="Arrow" Click="y_power_x_Click" Grid.Column="2"/>
        <Button x:Name="two_power" Content="xʸ" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="180,217,0,0" VerticalAlignment="Top" Width="85" Height="76" Foreground="White" Cursor="Arrow" Click="two_power_Click" Grid.Column="2"/>

        <Button x:Name="inverse" Content="¹⁄ₓ" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="10,294,0,0" VerticalAlignment="Top" Width="86" Height="75" Foreground="White" Cursor="Arrow" Click="inverse_Click" Grid.ColumnSpan="3"/>
        <Button x:Name="square_root" Content="²√x" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="7,294,0,0" VerticalAlignment="Top" Width="85" Height="75" Foreground="White" Cursor="Arrow" Click="square_root_Click" Grid.Column="2"/>
        <Button x:Name="cube_root" Content="³√x" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="93,294,0,0" VerticalAlignment="Top" Width="86" Height="75" Foreground="White" Cursor="Arrow" Click="cube_root_Click" Grid.Column="2"/>
        <Button x:Name="nth_root" Content="ʸ√x" FontSize="32px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="180,294,0,0" VerticalAlignment="Top" Width="85" Height="75" Foreground="White" Cursor="Arrow" Click="nth_root_Click" Grid.Column="2"/>
        <Button x:Name="natural_logarithm" Content="In" FontSize="26px" FontFamily="Arial Rounded MT Bold" BorderThickness="0" Background="#45424B" HorizontalAlignment="Left" Margin="266,294,0,0" VerticalAlignment="Top" Width="86" Height="75" Foreground="White" Cursor="Arrow" Click="natural_logarithm_Click" Grid.Column="2"/>
